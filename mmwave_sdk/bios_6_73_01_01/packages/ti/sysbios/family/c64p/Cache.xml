<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<gep name="C6x/Cache" title="C64x+ and 674x Cache" version="1">

<page name="Module" title="Module Settings">
<html><body><table>
  <tr>
      <td><form>
        <p>The Cache module provides C64x+ and C674x+ family-specific implementations of the
           APIs defined in the ti.sysbios.interfaces.ICache module. It also
           provides additional cache functions.</p>
        <p> </p>
      </form></td>
  </tr>
  <tr>
    <td><control type="checkbox" label="Add the Cache module to my configuration" value="value:this.$used" bold="true"/></td>
  </tr>
  <tr></tr>
  <tr><td>
    <h1>Cache Sizes</h1>
    <table>
        <tr>
          <td><control type="label" label="value:this.initSize.l1pSize.$summary" align="left"/></td>
          <td><control type="droplist"
               value="value:this.initSize.l1pSize"
               tooltip="value:this.initSize.l1pSize.$summary"/></td>
        </tr>
        <tr>
          <td><control type="label" label="value:this.initSize.l1dSize.$summary" align="left"/></td>
          <td><control type="droplist"
               value="value:this.initSize.l1dSize"
               tooltip="value:this.initSize.l1dSize.$summary"/></td>
        </tr>
        <tr>
          <td><control type="label" label="value:this.initSize.l2Size.$summary" align="left"/></td>
          <td><control type="droplist"
               value="value:this.initSize.l2Size"
               tooltip="value:this.initSize.l2Size.$summary"/></td>
        </tr>
    </table>
    </td><td>
    <h1>MAR Register Bitmasks</h1>
    <form><p>The bitmasks below are used to enable individual MAR registers.  Each bit maps to one of 32 unique MAR registers.  For example, if you set MAR 0 - 31 to 0x4, this will enable MAR region 2.</p></form>
    <table>
        <tr>
          <td><control type="label" label="MAR 0 - 31" align="left"/></td>
          <td><control type="editbox"
               display="hex"
               value="value:this.MAR0_31"
               tooltip="value:this.MAR0_31.$summary"/></td>
        </tr>
        <tr>
          <td><control type="label" label="MAR 32 - 63" align="left"/></td>
          <td><control type="editbox"
               display="hex"
               value="value:this.MAR32_63"
               tooltip="value:this.MAR32_63.$summary"/></td>
        </tr>
        <tr>
          <td><control type="label" label="MAR 64 - 95" align="left"/></td>
          <td><control type="editbox"
               display="hex"
               value="value:this.MAR64_95"
               tooltip="value:this.MAR64_95.$summary"/></td>
        </tr>
        <tr>
          <td><control type="label" label="MAR 96 - 127" align="left"/></td>
          <td><control type="editbox"
               display="hex"
               value="value:this.MAR96_127"
               tooltip="value:this.MAR96_127.$summary"/></td>
        </tr>
        <tr>
          <td><control type="label" label="MAR 128 - 159" align="left"/></td>
          <td><control type="editbox"
               display="hex"
               value="value:this.MAR128_159"
               tooltip="value:this.MAR128_159.$summary"/></td>
        </tr>
        <tr>
          <td><control type="label" label="MAR 160 - 191" align="left"/></td>
          <td><control type="editbox"
               display="hex"
               value="value:this.MAR160_191"
               tooltip="value:this.MAR160_191.$summary"/></td>
        </tr>
        <tr>
          <td><control type="label" label="MAR 192 - 223" align="left"/></td>
          <td><control type="editbox"
               display="hex"
               value="value:this.MAR192_223"
               tooltip="value:this.MAR192_223.$summary"/></td>
        </tr>
        <tr>
          <td><control type="label" label="MAR 224 - 255" align="left"/></td>
          <td><control type="editbox"
               display="hex"
               value="value:this.MAR224_255"
               tooltip="value:this.MAR224_255.$summary"/></td>
        </tr>
    </table>
    </td><td>
  </td></tr>
</table></body></html>
</page>
<properties name="Advanced" title="All Options" enabled="true" visible="true" link="this"/>
</gep>
