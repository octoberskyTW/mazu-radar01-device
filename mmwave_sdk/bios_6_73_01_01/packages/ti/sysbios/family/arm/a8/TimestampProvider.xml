<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<gep name="A8/Timestamp" title="A8 Timestamp Management" version="1">

<page name="Module" title="Module Settings">
<html><body><table>
  <tr>
      <td><form>
        <p>The TimestampProvider module provides device-specific time services for the <a href="http://rtsc.eclipse.org/cdoc-tip/index.html#xdc/runtime/Timestamp.html">Timestamp</a> module.</p>
        <p> </p>
      </form></td>
  </tr>
  <tr>
    <td><control type="checkbox" label="Add the A8-specific Timestamp management module to my configuration" value="value:this.$used" bold="true"/></td>
  </tr>
  <tr><td>
      <h1>Auto Refresh Enable</h1>
      <table>
        <tr>
          <td><control type="checkbox" 
               label="Enable auto refresh (Keeps the upper32 bits of the counter coherent)"
               value="value:this.autoRefreshEnable" 
               tooltip="value:this.autoRefreshEnable.$summary"/></td>
        </tr>
      </table>
  </td></tr>
</table></body></html>
</page>
</gep>

